<?xml version="1.0" encoding="utf-8"?>
<animated-selector
  xmlns:android="http://schemas.android.com/apk/res/android" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_pressed="true" android:id="@id/open" android:drawable="@drawable/$card_onboarding_trash_gray__0" />
    <item android:id="@id/closed" android:drawable="@drawable/$card_onboarding_trash_gray__1" />
    <transition android:drawable="@drawable/card_onboarding_trash_gray_opening" android:toId="@id/open" android:fromId="@id/closed" />
    <transition android:drawable="@drawable/card_onboarding_trash_gray_closing" android:toId="@id/closed" android:fromId="@id/open" />
</animated-selector>